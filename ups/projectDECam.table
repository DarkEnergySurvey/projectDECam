# despyastro loads: python, numpy, matplotlib and pyfits
setupRequired(despyastro 0.3.5+0) 

# DrawDEcam to draw ellipses
setupRequired(drawDECam 0.1.0+5)

# Pyfits
setupRequired(pyfits 3.3+2)

